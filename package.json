{
  "name": "@uktrade/wdio-image-diff-js",
  "version": "1.0.0",
  "description": "This package is visual regression tool intended to expose functions specifically for webdriverIO so that you can take screenshots and compare it to a baseline.",
  "main": "lib/index.js",
  "bin": {
    "wdio-image-diff": "bin/wdio-image-diff",
    "@uktrade/wdio-image-diff": "bin/wdio-image-diff"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest src/",
    "lint": "eslint --cache src/",
    "build": "babel src -d lib && cp src/reporter/template.hbs lib/reporter/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uktrade/wdio-image-diff.git"
  },
  "keywords": [
    "webdriverio",
    "saucelabs",
    "browserstack",
    "visual regression",
    "visual",
    "regression"
  ],
  "author": "UK GOV - DIT",
  "license": "MIT",
  "engines": {
    "yarn": "1.16.0",
    "node": "10.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^24.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0"
  },
  "bugs": {
    "url": "https://github.com/uktrade/wdio-image-diff/issues"
  },
  "homepage": "https://github.com/uktrade/wdio-image-diff#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "arg": "^4.1.1",
    "easyimage": "^3.1.1",
    "esm": "^3.2.25",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.1.2",
    "inquirer": "^7.0.0",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.4.0"
  },
  "resolutions": {
    "lodash": "^4.17.15",
    "mixin-deep": ">=2.0.1",
    "set-value": ">=3.0.1"
  }
